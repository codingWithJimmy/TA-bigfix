                    {
                        "input": [
                            "{{__response__.body}}", 
                            "$.result"
                        ], 
                        "output": "__stdout__", 
                        "method": "json_path"
                    }, 
                    {
                        "input": [
                            "{{__stdout__}}", 
                            "", 
                            "{{index}}", 
                            "{{host}}", 
                            "{{source}}", 
                            "{{sourcetype}}"
                        ], 
                        "output": "__stdout__", 
                        "method": "splunk_xml"
                    }, 
                    {
                        "input": [
                            "{{__stdout__}}"
                        ], 
                        "method": "std_output"
                    }
                ], 
                "skip_conditions": [
                    {
                        "input": [
                            "{{__response__.body}}", 
                            "$.result"
                        ], 
                        "method": "json_empty"
                    }
                ]
            }, 
            "pre_process": {
                "pipeline": [], 
                "skip_conditions": []
