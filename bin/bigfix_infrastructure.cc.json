{
    "requests": [
        {
            "iteration_mode": {
                "iteration_count": "1", 
                "stop_conditions": [
                    {
                        "input": [
                            "{{__response__.body}}", 
                            "$.result"
                        ], 
                        "method": "json_empty"
                    }
                ]
            }, 
            "post_process": {
