{
    "global_settings": {
        "logging": {
            "level": "{{__settings__.logging.loglevel}}"
        }
    }, 
    "requests": [
        {
            "post_process": {
                "pipeline": [
                    {
                        "input": [
                            "{{__response__.body}}", 
                            "$.result"
                        ], 
                        "method": "json_path", 
                        "output": "__stdout__"
                    }, 
                    {
                        "input": [
                            "{{__stdout__}}", 
                            "", 
                            "{{index}}", 
                            "{{host}}", 
                            "{{source}}", 
                            "{{sourcetype}}"
                        ], 
                        "method": "splunk_xml", 
                        "output": "__stdout__"
                    }, 
                    {
                        "input": [
                            "{{__stdout__}}"
                        ], 
                        "method": "std_output"
                    }
                ], 
                "skip_conditions": [
                    {
                        "input": [
                            "{{__response__.body}}", 
                            "$.result"
                        ], 
                        "method": "json_empty"
                    }
                ]
            }, 
            "iteration_mode": {
                "iteration_count": "1", 
                "stop_conditions": [
                    {
                        "input": [
                            "{{__response__.body}}", 
                            "$.result"
                        ], 
                        "method": "json_empty"
                    }
                ]
            }, 
            "request": {
                "headers": {}, 
                "auth": {
                    "type": "basic_auth", 
                    "options": {
                        "password": "{{global_account.password}}", 
                        "username": "{{global_account.username}}"
                    }
                }, 
                "url": "{{__settings__.additional_parameters.bigfix_server_url}}:{{__settings__.additional_parameters.bigfix_server_port}}/api/query?output=json&relevance=(%22analysis_name%3D%22%20%26%20item%201%20of%20it%2C%20%22property_name%3D%22%20%26%20item%201%20of%20item%200%20of%20it%2C%22property_value%3D%22%20%26%20item%200%20of%20item%200%20of%20item%200%20of%20it%2C%20%22host%3D%22%20%26%20item%201%20of%20item%200%20of%20item%200%20of%20it)%20of%20((((values%20of%20it%20%2C%20name%20of%20computer%20of%20it)%20of%20results%20of%20it%20%2C%20names%20of%20it)%20of%20properties%20of%20it)%20of%20it%20%2C%20names%20of%20it)%20of%20bes%20analysis%20whose%20(name%20of%20it%20is%20%22{{analysis_name}}%22)", 
                "method": "GET"
            }, 
            "pre_process": {
                "pipeline": [], 
                "skip_conditions": []
            }
        }
    ], 
    "tokens": [
        "__settings__.logging.loglevel", 
        "__settings__.additional_parameters.bigfix_server_url", 
        "__settings__.additional_parameters.bigfix_server_port", 
        "global_account.username", 
        "global_account.password", 
        "analysis_name"
    ], 
    "meta": {
        "apiVersion": "1.0.0"
    }
}