{
    "requests": [
        {
            "pre_process": {
                "pipeline": [], 
                "skip_conditions": []
            }, 
            "post_process": {
                "pipeline": [
                    {
                        "output": "__stdout__", 
                        "method": "json_path", 
                        "input": [
                            "{{__response__.body}}", 
                            "$.result"
                        ]
                    }, 
                    {
                        "output": "__stdout__", 
                        "method": "splunk_xml", 
                        "input": [
                            "{{__stdout__}}", 
                            "", 
                            "{{index}}", 
                            "{{host}}", 
                            "{{source}}", 
                            "{{sourcetype}}"
                        ]
                    }, 
                    {
                        "method": "std_output", 
                        "input": [
                            "{{__stdout__}}"
                        ]
                    }
                ], 
                "skip_conditions": [
                    {
                        "method": "json_empty", 
                        "input": [
                            "{{__response__.body}}", 
                            "$.result"
                        ]
                    }
                ]
            }, 
            "request": {
                "auth": {
                    "type": "basic_auth", 
                    "options": {
                        "username": "{{global_account.username}}", 
                        "password": "{{global_account.password}}"
                    }
                }, 
                "url": "{{__settings__.additional_parameters.bigfix_server_url}}:{{__settings__.additional_parameters.bigfix_server_port}}/api/query?output=json&relevance=%28%22nt_host%3D%22%26+item+0+of+it+as+string%2C+%22last_report_time%3D%22%26+item+1+of+it+as+string%2C+%22operating_system%3D%22+%26+item+2+of+it+as+string%2C+%22fixlet_id%3D%22+%26+item+7+of+item+3+of+it+as+string%2C+%22fixlet_name%3D%22+%26+item+0+of+item+3+of+it+as+string%2C+%22fixlet_category%3D%22+%26+item+3+of+item+3+of+it+as+string%2C+%22fixlet_type%3D%22+%26+item+6+of+item+3+of+it+as+string%2C+%22source%3D%22+%26+item+1+of+item+3+of+it+as+string%2C+%22source_id%3D%22+%26+item+8+of+item+3+of+it+as+string%2C+%22source_severity%3D%22+%26+item+2+of+item+3+of+it+as+string%2C+%22site%3D%22+%26+item+4+of+item+3+of+it+as+string%2C+%22is_custom_fixlet%3D%22+%26+item+5+of+item+3+of+it+as+string%29+of+%28name+of+it%2C+last+report+time+of+it%2C+operating+system+of+it%2C+%28name+of+it%2C+source+of+it%2C+source+severity+of+it%2C+category+of+it%2C+name+of+site+of+it%2C+custom+flag+of+it%2C+type+of+it%2C+id+of+it%2C+source+id+of+it%29+of+relevant+fixlets+whose+%28source+severity+of+it+is+not+%22nothing%22%29+of+it%29+of+bes+computers", 
                "method": "GET", 
                "headers": {}
            }, 
            "iteration_mode": {
                "stop_conditions": [
                    {
                        "method": "json_empty", 
                        "input": [
                            "{{__response__.body}}", 
                            "$.result"
                        ]
                    }
                ], 
                "iteration_count": "1"
            }
        }
    ], 
    "tokens": [
        "__settings__.logging.loglevel", 
        "__settings__.additional_parameters.bigfix_server_port", 
        "__settings__.additional_parameters.bigfix_server_url", 
        "global_account.username", 
        "global_account.password"
    ], 
    "global_settings": {
        "logging": {
            "level": "{{__settings__.logging.loglevel}}"
        }
    }, 
    "meta": {
        "apiVersion": "1.0.0"
    }
}