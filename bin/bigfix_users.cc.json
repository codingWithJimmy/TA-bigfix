{
    "requests": [
        {
            "request": {
                "auth": {
                    "options": {
                        "username": "{{global_account.username}}", 
                        "password": "{{global_account.password}}"
                    }, 
                    "type": "basic_auth"
                }, 
                "method": "GET", 
                "headers": {}, 
                "url": "{{__settings__.additional_parameters.bigfix_server_url}}:{{__settings__.additional_parameters.bigfix_server_port}}/api/query?output=json&relevance=(\"name=\" %26 name of it, \"master_operator=\" %26 master flag of it as string, \"action_count=\" %26 number of issued actions of it as string, \"creation_time=\" %26 creation time of it as string, \"last_login_time=\" %26 (if (exists last login time of it) then last login time of it as string else \"\")) of bes users"
            }, 
            "pre_process": {
                "skip_conditions": [], 
                "pipeline": []
            }, 
            "iteration_mode": {
                "stop_conditions": [
                    {
                        "method": "json_empty", 
                        "input": [
                            "{{__response__.body}}", 
                            "$.$.result[*]"
                        ]
                    }
                ], 
                "iteration_count": "1"
            }, 
            "post_process": {
                "skip_conditions": [
                    {
                        "method": "json_empty", 
                        "input": [
                            "{{__response__.body}}", 
                            "$.$.result[*]"
                        ]
                    }
                ], 
                "pipeline": [
                    {
                        "method": "json_path", 
                        "input": [
                            "{{__response__.body}}", 
                            "$.$.result[*]"
                        ], 
                        "output": "__stdout__"
                    }, 
                    {
                        "method": "splunk_xml", 
                        "input": [
                            "{{__stdout__}}", 
                            "", 
                            "{{index}}", 
                            "{{host}}", 
                            "{{source}}", 
                            "{{sourcetype}}"
                        ], 
                        "output": "__stdout__"
                    }, 
                    {
                        "method": "std_output", 
                        "input": [
                            "{{__stdout__}}"
                        ]
                    }
                ]
            }
        }
    ], 
    "meta": {
        "apiVersion": "1.0.0"
    }, 
    "global_settings": {
        "logging": {
            "level": "{{__settings__.logging.loglevel}}"
        }
    }, 
    "tokens": [
        "__settings__.logging.loglevel", 
        "__settings__.additional_parameters.bigfix_server_url", 
        "__settings__.additional_parameters.bigfix_server_port", 
        "global_account.username", 
        "global_account.password"
    ]
}